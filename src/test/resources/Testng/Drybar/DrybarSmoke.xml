<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Drybar Smoke Suite" parallel="false">

<listeners>
    <listener class-name="utils.TestListener"/>
</listeners>

     <test name="Drybar New Account Creation">
        <classes>
            <class name="tests.Drybar.DrybarCreateAccount"/>
        </classes>
    </test>
    
    <test name="Drybar Guest User Checkout">
        <classes>
            <class name="tests.Drybar.DrybarGuestCheckout"/>
        </classes>
    </test>
    <test name="Drybar Registered User Checkout">
        <classes>
            <class name="tests.Drybar.DrybarRegCheckout"/>
        </classes>
    </test>
    

</suite>